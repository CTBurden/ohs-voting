generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model players {
  id         Int     @id @default(autoincrement())
  first_name String? @map("first-name")
  last_name  String? @map("last-name")
  position   String?
  ranking    Int
}
